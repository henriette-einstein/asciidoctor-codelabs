<% if (node.getLevel() === 1) { %>
<google-codelab-step
  label="<%= node.getTitle() %>"
  <% if (node.hasAttribute('duration')) { %>
  duration="<%= node.getAttribute('duration') %>"
  <% } %>
>
    <%- node.getContent() %>
</google-codelab-step>
<% } else { %>
  <section>
    <h<%= node.getLevel() %>><%- node.getTitle() %></h<%= node.getLevel() %>>
   <%- node.getContent() %>
  </section>
<% } %>
